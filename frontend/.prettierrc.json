{
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "useTabs": false,
  "trailingComma": "all",
  "printWidth": 100,
  "arrowParens": "always",
  "endOfLine": "auto",
  "jsxBracketSameLine": false,
  "importOrder": [
    "^(react)(.*|$)",
    "^(styled-components)(.*|$)",
    "^(@/Router)(.*|$)",
    "^(@/Styles)(.*|$)",
    "^(\\./)(.*|$)",
    "^(@/Coach/pages)(.*|$)",
    "^(@/Crew/pages)(.*|$)",
    "^(@/Shared/pages)(.*|$)",
    "^(@/Coach/components)(.*|$)",
    "^(@/Crew/components)(.*|$)",
    "^(@/Shared/components)(.*|$)",
    "^(@/Coach/hooks)(.*|$)",
    "^(@/Crew/hooks)(.*|$)",
    "^(@/Shared/hooks)(.*|$)",
    "^(@/Coach/context)(.*|$)",
    "^(@/Crew/context)(.*|$)",
    "^(@/Shared/context)(.*|$)",
    "^(@/Coach/.*|$)",
    "^(@/Crew/.*|$)",
    "^(@/Shared/.*|$)",
    "^(@/Types)(.*|$)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
